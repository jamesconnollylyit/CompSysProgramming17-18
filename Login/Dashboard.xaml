<Window x:Class="Login.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login"
        mc:Ignorable="d"
        Title="Dashboard" Height="300" Width="300" WindowState="Maximized" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="btnAdmin" Content="Admin" Visibility="Collapsed" HorizontalAlignment="Left" Margin="25,10,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnNewItem" Visibility="Hidden" Content="New item" HorizontalAlignment="Left" Margin="117,10,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnNormalUser" Visibility="Visible" Content="Normal user" HorizontalAlignment="Left" Margin="207,10,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
