<Window x:Class="Fibonacci.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fibonacci"
        mc:Ignorable="d"
        Title="Fibonacci sequence" Height="213.889" Width="525" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="lblFibonacciMessage" Content="Please enter fibonacci sequence" HorizontalAlignment="Left" Margin="29,39,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbxMaxFibonacciNumber" HorizontalAlignment="Left" Height="23" Margin="235,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnGenerate" Content="Generate" HorizontalAlignment="Left" Margin="406,46,0,0" VerticalAlignment="Top" Width="75" Click="btnGenerate_Click"/>
        <TextBox x:Name="tbxFibonacciList" HorizontalAlignment="Left" Height="62" Margin="29,111,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="452"/>

    </Grid>
</Window>
